{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "worker": {
      "type": "application",
      "root": ".",
      "entryFile": "src/main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    }
  },
  "compilerOptions": {
    "deleteOutDir": true
  }
}
